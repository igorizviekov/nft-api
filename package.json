    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "ethers": "^5.6.9",
    "hardhat-gas-reporter": "^1.0.9",
    "install-peerdeps": "^3.0.3",
    "ipfs-http-client": "56.0.1",
    "node-fetch": "^3.3.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "solidity-coverage": "^0.8.2",
    "swagger-ui-express": "^4.3.0",
    "typechain": "^8.1.1",
    "typeorm": "^0.2.41",
